<Window x:Class="Client.EventLogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client" d:DataContext="{d:DesignInstance Type=local:EventLogViewModel}"
        mc:Ignorable="d"
        Title="EventLogWindow" Height="450" Width="800">
    <Grid>
        <DataGrid Margin="0" ColumnWidth="*" ItemsSource="{Binding EventLogs}" AutoGenerateColumns="True"/>
    </Grid>
</Window>
